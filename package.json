{
  "name": "cloudflare-kv-rate-limit",
  "version": "2.0.0",
  "description": "Cloudflare KV-based rate limiter.",
  "main": "./dist/cjs/CloudflareKVRateLimiter.js",
  "type": "module",
  "module": "./dist/esm/CloudflareKVRateLimiter.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/CloudflareKVRateLimiter.js",
      "require": "./dist/cjs/CloudflareKVRateLimiter.js",
      "default": "./dist/esm/CloudflareKVRateLimiter.js"
    }
  },
  "files": [
    "dist",
    "types",
    "CHANGELOG.md"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsup",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "prepare": "husky",
    "dev": "wrangler dev"
  },
  "author": "nswbmw",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nswbmw/cloudflare-kv-rate-limit.git"
  },
  "keywords": [
    "cloudflare",
    "kv",
    "ratelimit",
    "ratelimiter",
    "rate-limit",
    "limiter"
  ],
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "eslint": "9.39.1",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "neostandard": "0.12.2",
    "tsup": "8.5.0",
    "wrangler": "4.46.0"
  },
  "engines": {
    "node": ">=20"
  }
}
